<%= form_for @skatelog do |f| %>
  <div class="title">
    <%=f.label :title %>
    <%=f.text_field :title %>
  </div>

  <div class="seshtime">
    <%=f.label :seshtime %>
    <%=f.text_field :seshtime %>
  </div>

  <div class="tricks">
    <%=f.label :tricks %>
    <%=f.text_field :tricks %>
  </div>

  <div class="description">
    <%=f.label :description %>
    <%=f.text_area :description, size: '60x6' %>
  </div>

  <div class ="submit">
    <%=f.submit %>
  </div>
<% end %>
